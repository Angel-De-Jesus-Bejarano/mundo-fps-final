    <section class="main-formulario contenedor ">
    <div class="encabezado-formulario">
     <h1 class="texto-centrado">FORMULARIO EDICION REGISTRO</h1>
    </div>
     <div class="clase-principal">
         <fieldset title="Formulario">
             <legend>FORMULARIO CONTACTO</legend>
            <form class="contenido-insertar" action="/Edicion/<%= Alumno.id %>" method="post">

                    <label for="nombre">Nombre</label>
                    <input type="text" name="nombre" value="<%= Alumno.nombre %>" required >
                    <br>

                    <label for="aplellido">Apellido</label>
                    <input type="text" name="apellido" value="<%= Alumno.apellido %>" required>
                    <br>

                    <label for="pais">Pais</label>
                    <input type="text" name="pais" value="<%= Alumno.pais %>" required >
                    <br>

                    <label for="pais">Correo</label>
                    <input type="email" name="correo" value="<%= Alumno.correo %>"  required>
                    <br>

                    <label for="Comentario">Comentario</label>
                    <textarea name="comentario" value="<%= Alumno.comentario %>"  id="" cols="30" rows="10" required></textarea>
                    <br>

                    <script>
                        function alerta(){
                            alert('Datos enviados correctamente');
                        }
                    </script>
                    <input  type="submit" value="Enviar" class="boton-comunidad hover" onclick="alerta()">
            </form>  
        </fieldset>
      </div>
    </section>
   
